{
  "name": "fiyo/mattermost",
  "description":  "Php Wrapper to interact with Mattermost API https://api.mattermost.com/",
  "version": "1.3.0",
  "type": "library",
  "keywords": [
    "mattermost"
  ],
  "readme": "./README.md",
  "license": "MIT",
  "authors": [
    {
      "name": "Paul Celie",
      "email": "p.celie@fiyo.com",
      "homepage": "https://paul.celie.dev"
    }
  ],
  "require": {
    "php" : "^7.0 || ^7.1 || ^7.2 || ^7.3 || ^7.4 || ^8.0",
    "guzzlehttp/guzzle": "^6.3"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.2"
  },
  "autoload": {
    "psr-4": {
      "Fiyo\\Mattermost\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Fiyo\\Test\\": "test"
    }
  }
}
